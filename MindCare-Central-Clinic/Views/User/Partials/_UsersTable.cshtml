@model UserViewModel

@if (Model.UserList is null || Model.UserList.Count == 0)
{
    <p>Content not found.</p>
}
else
{
    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th scope="col">#</th>
                <th scope="col">Username</th>
                <th scope="col">Password</th>
                <th scope="col">Nível de Acesso</th>
                <th scope="col">Última atividade</th>
                <th scope="col">Icon here</th>
                <th scope="col">Icon here</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.UserList)
            {
                <tr>
                    <th scope="row">@item.Id</th>
                    <td>@item.Username</td>
                    <td>**********</td>
                    <td>@item.AccessLevel</td>
                    <td>@item.LastActivity</td>
                    <td>
                        @* <button class="button-6" onclick="ModalProfessional.Update(@item.Id, '@item.Name', '@item.Cpf', '@item.Gender', @item.Age)">Alterar</button> *@
                    </td>
                    <td>
                        @* <button class="button-6" onclick="ModalProfessional.Delete(@item.Id, '@item.Name')">Excluir</button> *@
                    </td>
                </tr>
            }
        </tbody>
    </table>
}